<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.nocompany.noscrabble.gui.StartseiteController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <center>
            <ImageView fx:id="iStartbild" fitHeight="612.0" fitWidth="621.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../images/Startbild.jpg" />
               </image>
            </ImageView>
         </center>
         <top>
            <MenuBar prefHeight="25.0" prefWidth="630.0" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="Spiel">
                  <items>
                        <MenuItem mnemonicParsing="false" text="Neues Spiel" />
                    <MenuItem mnemonicParsing="false" text="Spiel beenden" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Hilfe">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Spielregeln" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
